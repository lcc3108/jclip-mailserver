{
  "name": "test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/index.js",
    "prebuild": "rm -rf dist",
    "build": "NODE_ENV=production node -r ./env.js ./node_modules/.bin/babel src --out-dir dist --copy-files --extensions \".ts,.tsx\"",
    "postbuild": "cp -t ./dist package.json",
    "deploy": "terraform plan && terraform apply --auto-approve",
    "test": "NODE_ENV=test mocha -r ./mocha.js --timeout 10000",
    "test:all": "NODE_ENV=test mocha -r ./mocha.js --timeout 10000 ./src/test/**/*.spec.ts",
    "test:local": "NODE_ENV=test node -r ./mocha.js ./src/test/express.ts",
    "test:prod": "NODE_ENV=production node -r ./mocha.js ./src/test/express.ts",
    "test:prod:all": "NODE_ENV=production mocha -r ./mocha.js --timeout 10000 ./src/test/**/*.spec.ts",
    "test:local:watch": "NODE_ENV=test nodemon -r ./mocha.js ./src/test/express.ts --watch ./src -e ts",
    "lint": "tslint ./src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "apollo-server-cloud-functions": "^2.9.13",
    "apollo-server-lambda": "^2.9.12",
    "aws-serverless-express": "^3.3.6",
    "dotenv": "^8.0.0",
    "graphql": "^14.5.8",
    "morgan": "^1.9.1",
    "source-map-support": "^0.5.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.5.5",
    "@types/dotenv": "^6.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-minify": "^0.5.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.4",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  }
}
